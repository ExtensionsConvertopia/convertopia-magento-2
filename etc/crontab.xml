<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="convertopia_product_cron" instance="Innovadeltech\Convertopia\Cron\CronFileProduct" method="execute">
            <config_path>
                crontab/default/jobs/convertopia_product_cron/schedule/cron_expr
            </config_path>
        </job>
        <job name="convertopia_customer_cron" instance="Innovadeltech\Convertopia\Cron\CronFileCustomer" method="execute">
            <config_path>
                crontab/default/jobs/convertopia_customer_cron/schedule/cron_expr
            </config_path>
        </job>
        <job name="convertopia_order_cron" instance="Innovadeltech\Convertopia\Cron\CronFileOrder" method="execute">
            <config_path>
                crontab/default/jobs/convertopia_order_cron/schedule/cron_expr
            </config_path>
        </job>
        <job name="convertopia_product_delta" instance="Innovadeltech\Convertopia\Cron\CronDeltaProduct" method="execute">
            <config_path>
                crontab/default/jobs/convertopia_product_delta/schedule/cron_delta
            </config_path>
        </job>
        <job name="convertopia_customer_delta" instance="Innovadeltech\Convertopia\Cron\CronDeltaCustomer" method="execute">
            <config_path>
                crontab/default/jobs/convertopia_customer_delta/schedule/cron_delta
            </config_path>
        </job>
        <job name="convertopia_order_delta" instance="Innovadeltech\Convertopia\Cron\CronDeltaOrder" method="execute">
            <config_path>
                crontab/default/jobs/convertopia_order_delta/schedule/cron_delta
            </config_path>
        </job>                
    </group>

</config>