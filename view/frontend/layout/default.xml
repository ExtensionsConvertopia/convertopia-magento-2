<?xml version="1.0"?>
<page layout="1column"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="head.additional">
            <block class="Innovadeltech\Convertopia\Block\PackageDelay" name="convertopia.tracking.add_to_cart" template="Innovadeltech_Convertopia::track/add_to_cart.phtml" before="-" />
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" template="Innovadeltech_Convertopia::convertopia.phtml" />
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="viewData" template="Innovadeltech_Convertopia::products.phtml" after="-" />
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Innovadeltech\Convertopia\Block\ProductTilesShow" name="ProductTilesShow" template="Innovadeltech_Convertopia::ProductTilesShow.phtml" after="-" />
        </referenceContainer>
    </body>
</page>



